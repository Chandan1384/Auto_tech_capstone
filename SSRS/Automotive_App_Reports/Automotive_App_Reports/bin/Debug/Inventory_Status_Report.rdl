  <DataSets>
    <DataSet Name="Main_ds">
      <Query>
        <DataSourceName>main_ds</DataSourceName>
        <CommandText>SELECT 
    I.location_id, 
    I.location_name, 
    I.location_type, 
    I.country, 
    p.category, 
    p.product_id, 
    p.product_name, 
    im.current_stock, 
    im.reorder_level, 
    im.max_stock_level, 
    im.safety_stock, 
    im.last_movement_date, 
    im.unit_cost, 
    im.current_stock * im.unit_cost AS inventory_value
FROM 
    inventory_master im
JOIN 
    products p ON p.product_id = im.product_id
JOIN 
    locations I ON I.location_id = im.location_id
ORDER BY 
    I.location_name, 
    p.category, 
    p.product_name</CommandText>
      </Query>
